    "/v1/documents": {
      "post": {
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "tenantId": {
                    "description": "Tenant ID to scope the search",
                    "example": "tenant123",
                    "type": "string",
                    "minLength": 1
                  },
                  "title": {
                    "description": "Title of the document, provided by the user",
                    "example": "My Document",
                    "type": "string",
                    "minLength": 1
                  },
                  "source": {
                    "description": "source of the document",
                    "example": "upload",
                    "default": "upload",
                    "type": "string",
                    "enum": [
                      "upload",
                      "url",
                      "api"
                    ]
                  },
                  "mimeType": {
                    "description": "MIME type of the document",
                    "example": "application/pdf",
                    "type": "string"
                  },
                  "content": {
                    "description": "Document content",
                    "example": "Test content",
                    "type": "string"
                  }
                },
                "required": [
                  "tenantId",
                  "title"
                ]
              }
            }
          }
        },
        "responses": {
          "202": {
            "description": "Queued",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "description": "document Id, assigned by the server",
                      "example": "doc123",
                      "type": "string",
                      "minLength": 1
                    },
                    "status": {
                      "default": "queued",
                      "type": "string",
                      "enum": [
                        "indexed",
                        "queued",
                        "failed",
                        "processing"
                      ]
                    }
                  },
                  "required": [
                    "id",
                    "status"
                  ],
                  "additionalProperties": false
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "object",
                      "properties": {
                        "code": {
                          "type": "string"
                        },
                        "message": {
                          "type": "string"
                        },
                        "details": {},
                        "requestId": {
                          "anyOf": [
                            {
                              "type": "string"
                            },
                            {
                              "type": "number"
                            }
                          ]
                        }
                      },
                      "required": [
                        "code",
                        "message"
                      ],
                      "additionalProperties": false
                    }
                  },
                  "required": [
                    "error"
                  ],
                  "additionalProperties": false
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "object",
                      "properties": {
                        "code": {
                          "type": "string"
                        },
                        "message": {
                          "type": "string"
                        },
                        "details": {},
                        "requestId": {
                          "anyOf": [
                            {
                              "type": "string"
                            },
                            {
                              "type": "number"
                            }
                          ]
                        }
                      },
                      "required": [
                        "code",
                        "message"
                      ],
                      "additionalProperties": false
                    }
                  },
                  "required": [
                    "error"
                  ],
                  "additionalProperties": false
                }
              }
            }
          }
        }
      }
    },
    "/v1/search": {
      "get": {
        "parameters": [
          {
            "in": "query",
            "name": "tenantId",
            "schema": {
              "description": "Tenant ID to scope the search",
              "example": "tenant123",
              "type": "string",
              "minLength": 1
            },
            "required": true,
            "description": "Tenant ID to scope the search"
          },
          {
            "in": "query",
            "name": "q",
            "schema": {
              "example": "typescript",
              "type": "string",
              "minLength": 1
            },
            "required": true
          },
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "description": "Maximum number of results to return",
              "example": 10,
              "default": 10,
              "type": "integer",
              "minimum": 1,
              "maximum": 50
            },
            "description": "Maximum number of results to return"
          },
          {
            "in": "query",
            "name": "offset",
            "schema": {
              "description": "Number of results to skip for pagination",
              "example": 0,
              "default": 0,
              "type": "integer",
              "minimum": 0,
              "maximum": 9007199254740991
            },
            "description": "Number of results to skip for pagination"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "total": {
                      "description": "Total number of matched items",
                      "example": 100,
                      "type": "integer",
                      "minimum": 0,
                      "maximum": 9007199254740991
                    },
                    "items": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "description": "A unique identifier string",
                            "example": "abc123",
                            "type": "string",
                            "minLength": 1
                          },
                          "title": {
                            "example": "typescript documentation 1",
                            "type": "string"
                          },
                          "snippet": {
                            "description": "A short snippet of the matched content",
                            "example": "TypeScript is a typed superset of JavaScript that compiles to plain JavaScript...",
                            "type": "string"
                          },
                          "score": {
                            "description": "Relevance score for ranking",
                            "example": 0.95,
                            "type": "number"
                          },
                          "url": {
                            "description": "Link to the full document or resource",
                            "example": "https://www.typescriptlang.org/docs/",
                            "type": "string"
                          }
                        },
                        "required": [
                          "id",
                          "title"
                        ],
                        "additionalProperties": false
                      }
                    },
                    "page": {
                      "description": "Page number, starting from 1",
                      "example": 1,
                      "default": 1,
                      "type": "integer",
                      "minimum": 1,
                      "maximum": 9007199254740991
                    },
                    "pageSize": {
                      "description": "Number of items per page, max 100",
                      "example": 10,
                      "default": 10,
                      "type": "integer",
                      "minimum": 1,
                      "maximum": 100
                    }
                  },
                  "required": [
                    "total",
                    "items"
                  ],
                  "additionalProperties": false
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "object",
                      "properties": {
                        "code": {
                          "type": "string"
                        },
                        "message": {
                          "type": "string"
                        },
                        "details": {},
                        "requestId": {
                          "anyOf": [
                            {
                              "type": "string"
                            },
                            {
                              "type": "number"
                            }
                          ]
                        }
                      },
                      "required": [
                        "code",
                        "message"
                      ],
                      "additionalProperties": false
                    }
                  },
                  "required": [
                    "error"
                  ],
                  "additionalProperties": false
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "object",
                      "properties": {
                        "code": {
                          "type": "string"
                        },
                        "message": {
                          "type": "string"
                        },
                        "details": {},
                        "requestId": {
                          "anyOf": [
                            {
                              "type": "string"
                            },
                            {
                              "type": "number"
                            }
                          ]
                        }
                      },
                      "required": [
                        "code",
                        "message"
                      ],
                      "additionalProperties": false
                    }
                  },
                  "required": [
                    "error"
                  ],
                  "additionalProperties": false
                }
              }
            }
          }
        }
      }
    },
